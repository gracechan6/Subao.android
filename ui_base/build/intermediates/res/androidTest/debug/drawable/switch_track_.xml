<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="20dp"/>
            <solid android:color="@color/switch_checked_"/>
            <size android:height="@dimen/switch_height_"
                  android:width="@dimen/switch_width_"/>
        </shape>
    </item>

    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/switch_height_"/>
            <solid android:color="@color/white_"/>
            <size android:height="@dimen/switch_height_"
                  android:width="@dimen/switch_width_"/>
            <stroke android:width="1dp"
                    android:color="@color/switch_stoke_"/>
        </shape>
    </item>
</selector><!-- From: file:/C:/Users/ASUS/Desktop/工作安排/20150803/Subao.android/ui_base/src/main/res/drawable/switch_track_.xml --><!-- From: file:/C:/Users/ASUS/Desktop/工作安排/20150803/Subao.android/ui_base/build/intermediates/bundles/debug/res/drawable/switch_track_.xml -->