<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="20dp"/>
            <solid android:color="@color/switch_checked_"/>
            <size android:height="@dimen/switch_height_"
                  android:width="@dimen/switch_width_"/>
        </shape>
    </item>

    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/switch_height_"/>
            <solid android:color="@color/white_"/>
            <size android:height="@dimen/switch_height_"
                  android:width="@dimen/switch_width_"/>
            <stroke android:width="1dp"
                    android:color="@color/switch_stoke_"/>
        </shape>
    </item>
</selector><!-- From: file:/Users/michael/Documents/JinwangSVN/产品/消息中心/源代码/安卓客户端/JWMsgCenter/jinwanguibase/src/main/res/drawable/switch_track_.xml --><!-- From: file:/Users/michael/Documents/JinwangSVN/产品/消息中心/源代码/安卓客户端/JWMsgCenter/jinwanguibase/build/intermediates/bundles/debug/res/drawable/switch_track_.xml -->